{
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "paths": {
            "~*": ["*"],
            "/*": ["*"],
            "/static/*": ["static/*"],
            "/node_modules/*": ["node_modules/*", "node_modules/@types/*"]
        },
    },
}